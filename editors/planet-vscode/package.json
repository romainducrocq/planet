{
    "name": "planet-vscode",
    "displayName": "Planet",
    "description": "Planet Language",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "planet",
            "aliases": ["Planet", "planet"],
            "extensions": [".plx",".plx.m4",".etc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "planet",
            "scopeName": "source.planet",
            "path": "./syntaxes/planet.tmLanguage.json"
        }]
    }
}
