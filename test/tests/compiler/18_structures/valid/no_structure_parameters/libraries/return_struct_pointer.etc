#  Test returning struct pointers from functions
#  * and using struct pointers returned from functions
#  * 

use `stdlib`

#  define some struct types
type struc inner(    d: f64    
    , i: i32    )

type struc outer(    a: char    , b: char    
    , substruct: struc inner    )

type struc outermost(    i: i32    , nested_ptr: *struc outer    , nested_struct: struc outer    )

#  declare some functions that return pointers to structs
pub fn make_struct_inner(seed: 
    i32) *struc inner;
pub fn make_struct_outer(seed: i32) *struc outer;
pub fn make_struct_outermost(seed: i32) *struc outermost;
