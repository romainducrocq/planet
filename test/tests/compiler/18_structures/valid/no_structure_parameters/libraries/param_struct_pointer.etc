#  Test passing struct pointers as function parameters 

#  library function
use "stdlib"

#  define some struct types
type struc inner(    d: f64    
    , i: i32    )

type struc outer(    a: char    , b: char    
    , substruct: struc inner    )

#  library function declarations
pub fn access_members_through_pointer(ptr: *struc outer, expected_a: bool, expected_b: i32, expected_d: f64, expected_i: 
    i32) i32;



pub fn update_members_through_pointer(ptr: *struc outer, a: i32, b: bool, inner_ptr: *struc inner) none;

