#  Test initialization of non-nested structs with automatic storage duration,
#  * including:
#  * - partial initialization
#  * - implicit type conversions
#  * - compound and single expressions as initializers
#  * - string literals as pointer and array initializers
#  * 









#  library functions
pub fn strcmp(s1: string, s2: *char) i32;

pub fn malloc(size: u64) *any;
pub fn calloc(nmemb: u64, size: u64) *any;

#  struct type def
type struc s(    one_msg: *char    
    , two_arr: [3]u8    , three_self_ptr: *struc s    
    , four_d: f64    , five_d_ptr: *f64    
    )

#  validation functions defined in library
pub fn validate_full_initialization(ptr: *struc s) i32;
pub fn validate_partial_initialization(ptr: *struc s, expected_msg: *char) i32;
pub fn validate_converted(ptr: *struc s) i32;
pub fn validate_two_structs(ptr1: *struc s, ptr2: *struc s) i32;
