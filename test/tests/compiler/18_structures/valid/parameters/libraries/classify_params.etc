#  Test that we classify structure parameters correctly,
#  * by passing a variety of structures as arguments.
#  * Each test function takes only one argument.
#  * 









use "string"

#  from Listing 18-39
type struc twelve_bytes(    i: i32    , arr: [8]char    ) #  two INTEGER eightbytes

#  from Listing 18-40
type struc inner(    i: i32    , ch2: char    )

type struc nested_ints(    ch1: char    
    , nested: struc inner    ) #  two INTEGER eightbytes

#  from Listing 18-41
type struc flattened_ints(    c: char    , 
    i: i32    , a: char    
    ) #  two INTEGER eightbytes

#  From uncaptioned listing in "Classifying Eightbytes" section
type struc large(    i: i32    , d: f64    
    , arr: [10]char    ) #  four MEMORY eightbytes

#  Three structure declarations from Listing 18-42
type struc two_ints(    i: i32    , i2: i32    ) #  one INTEGER eightbyte

type struc nested_double(
    array: [1]f64    
    ) #  one SSE eightbyte

type struc two_eightbytes(    d: f64    , c: char    ) #  one SSE eightbyte, one INTEGER eightbyte

#  From Listing 18-47
type struc pass_in_memory(    w: f64    , x: f64    
    , y: i32    , z: i64    ) #  four MEMORY eightbytes

#  validation functions defined in library
pub fn test_twelve_bytes(s: struc twelve_bytes) i32;
pub fn test_nested_ints(s: struc nested_ints) i32;
pub fn test_flattened_ints(s: struc flattened_ints) i32;
pub fn test_large(s: struc large) i32;
pub fn test_two_ints(s: struc two_ints) i32;
pub fn test_nested_double(s: struc nested_double) i32;
pub fn test_two_eightbytes(s: struc two_eightbytes) i32;
pub fn test_pass_in_memory(s: struc pass_in_memory) i32;
